<Window x:Class="catteryConsole.AfterLoginWPF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:catteryConsole"
        mc:Ignorable="d"
        Title="AfterLoginWPF" Height="450" Width="800">
    <Grid>
        <Label Content="Benvenuto Al Gattile Online" HorizontalAlignment="Center" Margin="0,25,0,0" VerticalAlignment="Top" Width="646" FontWeight="Bold" FontSize="48" Height="69"/>
        <Button x:Name="btn_AdminPanel" Content="Aggiungi Gatti" HorizontalAlignment="Center" Margin="0,159,0,0" VerticalAlignment="Top" Height="58" Width="252" />
        <Button x:Name="btn_ViewCats" Content="Visualizza Gatti" HorizontalAlignment="Center" Margin="0,325,0,0" VerticalAlignment="Top" Height="58" Width="252" Click="btn_ViewCats_Click"/>
        <Button x:Name="btn_ViewAdoptions" Content="Visualizza Adozioni" HorizontalAlignment="Center" Margin="0,238,0,0" VerticalAlignment="Top" Height="58" Width="252" Click="btn_ViewAdoptions_Click" />

    </Grid>
</Window>
